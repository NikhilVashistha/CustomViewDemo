<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffd9d9d9" android:endColor="#ffd9d9d9" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffd9d9d9" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#80ffd300" android:endColor="#a0ffcb00" android:angle="270.0" android:centerY="0.75" android:centerColor="#80ffb600" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff55b8f6" android:endColor="#ff55b8f6" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff55b8f6" />
            </shape>
        </clip>
    </item>
</layer-list>